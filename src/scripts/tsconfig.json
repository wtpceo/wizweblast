{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "baseUrl": "..",
    "paths": {
      "@/*": ["*"]
    }
  }
} 